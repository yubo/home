Include conf.d/*

Host *
	ServerAliveInterval 60
	ServerAliveCountMax 2
	ControlMaster auto
	ControlPath ~/.ssh/master-%r@%h:%p
	StrictHostKeyChecking no
	HostkeyAlgorithms +ssh-rsa
	#PubkeyAcceptedAlgorithms +ssh-rsa

Host github.com
	identityFile ~/.ssh/id_rsa

Host github.com
	Hostname ssh.github.com
	Port 443
	User git

